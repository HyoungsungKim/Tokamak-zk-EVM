{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug trusted-setup (release)",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": ["build", "--release", "-p", "trusted-setup"],
        "filter": { "name": "trusted-setup", "kind": "bin" }
      },
      "program": "${cargo:program}",
      "args": [
        "${workspaceFolder}/../frontend/qap-compiler/subcircuits/library",
        "${workspaceFolder}/../frontend/synthesizer/examples/outputs",
        "${workspaceFolder}/setup/trusted-setup/output"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "DYLD_LIBRARY_PATH": "${workspaceFolder}/../../dist/macOS/backend-lib/icicle/lib",
        "LD_LIBRARY_PATH": "${workspaceFolder}/../../dist/linux22/backend-lib/icicle/lib"
      },  
      "sourceLanguages": ["rust"]
    },
    {
